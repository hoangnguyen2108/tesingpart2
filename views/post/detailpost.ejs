<%- include('../_partial/_header'); -%>

<div class="row">
    <div class="col-md-6">
        <h1><%= post.title %></h1>
    </div>
</div>


<div class="row">
   <div class="col-md-12">
       <div class="panel panel-default">
          <div class="panel-body">
            <p> Phone Number:</p>
              <%= post.content %>            
          </div>
                    
       </div>
   </div>
   <div class="col-md-12">
    <div class="panel panel-default">
       <div class="panel-body">
           <p> Email Address:</p>
           <%= post.content1 %>            
       </div>
                 
    </div>
    
    <button type="cancel" class="btn btn-default" href="#" role="button"><a href="/posts/">Back to the Contact List</a></button>
</div>
</div>


<%- include ('../_partial/_footer'); -%>